{
  "name": "react_todo",
  "version": "0.0.1",
  "description": "React Todo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catalin-testenv/react_todolist.git"
  },
  "author": "Catalin Enache",
  "license": "MIT",
  "homepage": "https://github.com/catalin-testenv/react_todolist.git",
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.3.0",
    "envify": "4.0.0",
    "http-server": "latest",
    "react-test-renderer": "15.5.4",
    "uglify-js": "3.0.4",
    "uglifyify": "3.0.4"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build:libs:dev": "browserify -r react -r react-dom -r prop-types -o dist/vendor.js",
    "build:app:dev": "NODE_PATH=./src browserify -x react -x react-dom -x prop-types -o dist/bundle.js src/app.js",
    "serve": "node node_modules/http-server/bin/http-server -a 0.0.0.0 -p 8001 -c-1"
  }
}
